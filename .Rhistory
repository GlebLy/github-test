etoFactor <- function(x){
  if(x<2){
    return(FALSE);
  }else{
    flag <- 2;
    while (flag<x){
      if(x%%flag==0){
        return(FALSE);
        }
      flag <- flag + 1;
     }
     return(TRUE);
   }
}
listFactorov <- function(predel){
  if (x<2){
    return(NULL);
  }else if (x == 2) {
    return(x);
   } else {
    l <- 2:x;
    i <- 0;
    while (i<predel){
      if (etoFactor(i)){
        #print(i);
        l <- l[!l%%i==0];
      }
      i <- i + 1;
     }
 }
 return(l);
}
